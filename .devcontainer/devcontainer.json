{
  "name": "Netbox Plugin Development Container",
  "dockerComposeFile": "./docker-compose.yml",
  "service": "netbox",
  "workspaceFolder": "/workspace",  
  "postStartCommand": "chmod o+x /workspace/.devcontainer/startup.sh && /workspace/.devcontainer/startup.sh",
  "features": {
    "ghcr.io/devcontainers/features/python:1": {
      "version": "3.12"
    }
  },
  "customizations": {
    "vscode": {
      "settings": {},
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter"
      ]
    }
  }
}
